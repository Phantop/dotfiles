#!/bin/bash
cd $D/Games/Minecraft
rm `fd -e log`
bin/MultiMC -d . $@
rm functions/*
for i in `ls -d instances/*/ | grep -v _MMC_TEMP | xargs -n1 basename`
do
    echo  > functions/$i.fish "function $i
    minecraft -l $i; end"
done
for i in `cat ~/.config/qutebrowser/quickmarks | cut -d ' ' -f1`
do
    echo > functions/$i.fish function $i"
    qb / \":quickmark-load $i\"; end"
done
